{
  "name": "@my-react/react-dom",
  "description": "@my-react/react-dom",
  "author": "MrWangJustToDo",
  "license": "MIT",
  "version": "0.2.2",
  "main": "index.js",
  "types": "./dist/types/index.d.ts",
  "unpkg": "./dist/umd/index.development.js",
  "jsdelivr": "./dist/umd/index.development.js",
  "files": [
    "dist",
    "client.js",
    "server.js",
    "server.node.js",
    "server.browser.js",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MrWangJustToDo/MyReact",
    "directory": "packages/myreact-dom"
  },
  "homepage": "https://mrwangjusttodo.github.io/MyReact",
  "exports": {
    ".": {
      "require": "./index.js",
      "node": "./index.js",
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./client": {
      "require": "./client.js",
      "node": "./client.js",
      "types": "./client.d.ts",
      "default": "./client.js"
    },
    "./server": {
      "require": "./server.js",
      "node": "./server.js",
      "types": "./server.d.ts",
      "default": "./server.js"
    },
    "./server.browser": {
      "require": "./server.browser.js",
      "node": "./server.browser.js",
      "types": "./server.d.ts",
      "default": "./server.browser.js"
    },
    "./server.node": {
      "require": "./server.node.js",
      "node": "./server.node.js",
      "types": "./server.d.ts",
      "default": "./server.node.js"
    },
    "./package.json": "./package.json"
  },
  "buildOptions": {
    "input": "./src/index.ts",
    "output": [
      {
        "dir": "./dist",
        "entryFileNames": "cjs/index.js",
        "format": "cjs",
        "multiple": true,
        "sourcemap": true
      },
      {
        "dir": "./dist",
        "entryFileNames": "esm/index.js",
        "format": "esm",
        "sourcemap": true
      },
      {
        "dir": "./dist",
        "entryFileNames": "umd/index.js",
        "format": "umd",
        "multiple": true,
        "globals": {
          "@my-react/react": "React"
        },
        "name": "ReactDOM",
        "sourcemap": true
      }
    ]
  },
  "keywords": [
    "react-dom",
    "react-dom like",
    "react-dom server",
    "react-dom client"
  ],
  "dependencies": {
    "@my-react/react-shared": "latest",
    "@my-react/react-reconciler": "latest"
  },
  "peerDependencies": {
    "@my-react/react": ">=0.2.1"
  }
}
