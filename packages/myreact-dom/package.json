{
  "name": "@my-react/react-dom",
  "description": "@my-react/react-dom",
  "version": "0.1.5",
  "main": "index.js",
  "types": "./dist/types/index.d.ts",
  "unpkg": "./dist/umd/index.development.js",
  "files": [
    "dist",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MrWangJustToDo/MyReact/tree/main/packages/myreact-dom"
  },
  "exports": {
    ".": {
      "require": "./index.js",
      "node": "./index.js",
      "default": "./index.js"
    },
    "./client": {
      "require": "./index.js",
      "node": "./index.js",
      "default": "./index.js"
    },
    "./server": {
      "require": "./index.js",
      "node": "./index.js",
      "default": "./index.js"
    }
  },
  "buildOptions": {
    "input": "./src/index.ts",
    "output": [
      {
        "dir": "./dist",
        "entryFileNames": "cjs/index.js",
        "format": "cjs",
        "multiple": true,
        "sourcemap": true
      },
      {
        "dir": "./dist",
        "entryFileNames": "esm/index.js",
        "format": "esm",
        "sourcemap": true
      },
      {
        "dir": "./dist",
        "entryFileNames": "umd/index.js",
        "format": "umd",
        "multiple": true,
        "globals": {
          "@my-react/react": "React",
          "@my-react/react-reactive": "ReactReactive"
        },
        "name": "ReactDOM",
        "sourcemap": true
      }
    ]
  },
  "keywords": [
    "react-dom",
    "react-dom like"
  ],
  "dependencies": {
    "@my-react/react": "latest",
    "@my-react/react-shared": "latest",
    "@my-react/react-reconciler": "latest"
  }
}
