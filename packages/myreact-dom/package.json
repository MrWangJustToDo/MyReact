{
  "name": "@my-react/react-dom",
  "description": "@my-react/react-dom",
  "author": "MrWangJustToDo",
  "license": "MIT",
  "version": "0.2.5",
  "main": "index.js",
  "types": "./dist/types/index.d.ts",
  "unpkg": "./dist/umd/index.development.js",
  "jsdelivr": "./dist/umd/index.development.js",
  "files": [
    "dist",
    "client.js",
    "server.js",
    "server.node.js",
    "server.browser.js",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MrWangJustToDo/MyReact",
    "directory": "packages/myreact-dom"
  },
  "homepage": "https://mrwangjusttodo.github.io/MyReact",
  "exports": {
    ".": {
      "require": "./index.js",
      "node": "./index.js",
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./client": {
      "require": "./client.js",
      "node": "./client.js",
      "types": "./client.d.ts",
      "default": "./client.js"
    },
    "./server": {
      "require": "./server.js",
      "node": "./server.js",
      "types": "./server.d.ts",
      "default": "./server.js"
    },
    "./server.browser": {
      "require": "./server.browser.js",
      "node": "./server.browser.js",
      "types": "./server.d.ts",
      "default": "./server.browser.js"
    },
    "./server.node": {
      "require": "./server.node.js",
      "node": "./server.node.js",
      "types": "./server.d.ts",
      "default": "./server.node.js"
    },
    "./package.json": "./package.json"
  },
  "buildOptions": [
    {
      "input": "./src/index.ts",
      "pkgName": "client",
      "output": [
        {
          "dir": "./dist",
          "entryFileNames": "cjs/client.js",
          "format": "cjs",
          "multiple": true,
          "sourcemap": true
        },
        {
          "dir": "./dist",
          "entryFileNames": "cjs/index.js",
          "format": "cjs",
          "multiple": true,
          "sourcemap": true
        },
        {
          "dir": "./dist",
          "entryFileNames": "esm/client.js",
          "format": "esm",
          "sourcemap": true
        },
        {
          "dir": "./dist",
          "entryFileNames": "esm/index.js",
          "format": "esm",
          "sourcemap": true
        },
        {
          "dir": "./dist",
          "entryFileNames": "umd/client.js",
          "format": "umd",
          "multiple": true,
          "globals": {
            "@my-react/react": "React"
          },
          "name": "ReactDOM",
          "sourcemap": true
        },
        {
          "dir": "./dist",
          "entryFileNames": "umd/index.js",
          "format": "umd",
          "multiple": true,
          "globals": {
            "@my-react/react": "React"
          },
          "name": "ReactDOM",
          "sourcemap": true
        }
      ]
    },
    {
      "input": "./src/server/index.ts",
      "pkgName": "server",
      "output": [
        {
          "dir": "./dist",
          "entryFileNames": "cjs/server.js",
          "format": "cjs",
          "multiple": true,
          "sourcemap": true
        },
        {
          "dir": "./dist",
          "entryFileNames": "esm/server.js",
          "format": "esm",
          "sourcemap": true
        },
        {
          "dir": "./dist",
          "entryFileNames": "umd/server.js",
          "format": "umd",
          "multiple": true,
          "globals": {
            "@my-react/react": "React"
          },
          "name": "ReactServerDOM",
          "sourcemap": true
        }
      ]
    }
  ],
  "keywords": [
    "react-dom",
    "react-dom like",
    "react-dom server",
    "react-dom client"
  ],
  "devDependencies": {
    "@my-react/react-shared": "^0.2.5",
    "@my-react/react-reconciler": "^0.2.5"
  },
  "peerDependencies": {
    "@my-react/react": ">=0.2.5"
  }
}
