{
  "name": "@my-react/react-dom",
  "description": "@my-react/react-dom",
  "version": "0.0.8",
  "main": "index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MrWangJustToDo/MyReact/tree/main/packages/myreact-dom"
  },
  "exports": {
    ".": {
      "require": "./index.js",
      "node": "./index.js",
      "default": "./index.js"
    },
    "./client": {
      "require": "./index.js",
      "node": "./index.js",
      "default": "./index.js"
    },
    "./server": {
      "require": "./index.js",
      "node": "./index.js",
      "default": "./index.js"
    }
  },
  "buildOptions": {
    "input": "./src/index.ts",
    "output": [
      {
        "dir": "./dist",
        "entryFileNames": "cjs/index.js",
        "format": "cjs",
        "multiple": true,
        "globals": {
          "@my-react/react": "React"
        },
        "sourcemap": true
      },
      {
        "dir": "./dist",
        "entryFileNames": "esm/index.js",
        "format": "esm",
        "globals": {
          "@my-react/react": "React"
        },
        "sourcemap": true
      },
      {
        "dir": "./dist",
        "entryFileNames": "umd/index.js",
        "format": "umd",
        "multiple": true,
        "globals": {
          "@my-react/react": "React"
        },
        "name": "ReactDOM",
        "sourcemap": true
      }
    ]
  },
  "dependencies": {
    "@my-react/react": "latest",
    "@my-react/react-shared": "latest",
    "@my-react/react-reconciler": "latest",
    "lodash": "^4.17.21"
  }
}
