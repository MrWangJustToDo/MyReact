{
  "name": "@my-react/react-dom",
  "description": "@my-react/react-dom",
  "author": "MrWangJustToDo",
  "license": "MIT",
  "version": "0.3.10",
  "main": "index.js",
  "unpkg": "./dist/umd/index.development.js",
  "jsdelivr": "./dist/umd/index.development.js",
  "files": [
    "dist",
    "client.js",
    "server.js",
    "server.node.js",
    "server.browser.js",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MrWangJustToDo/MyReact",
    "directory": "packages/myreact-dom"
  },
  "homepage": "https://mrwangjusttodo.github.io/MyReact",
  "exports": {
    ".": "./index.js",
    "./client": "./client.js",
    "./server": "./server.js",
    "./server.browser": "./server.browser.js",
    "./server.node": "./server.node.js",
    "./package.json": "./package.json"
  },
  "buildOptions": [
    {
      "input": "./src/index.ts",
      "pkgName": "client",
      "output": [
        {
          "dir": "./dist",
          "entryFileNames": "cjs/index.js",
          "format": "cjs",
          "type": true,
          "multiple": true
        },
        {
          "dir": "./dist",
          "entryFileNames": "umd/index.js",
          "format": "umd",
          "multiple": true,
          "globals": {
            "@my-react/react": "React"
          },
          "name": "ReactDOM"
        }
      ]
    },
    {
      "input": "./src/server/index.ts",
      "pkgName": "server",
      "output": [
        {
          "dir": "./dist",
          "entryFileNames": "cjs/server.js",
          "format": "cjs",
          "type": true,
          "multiple": true
        },
        {
          "dir": "./dist",
          "entryFileNames": "umd/server.js",
          "format": "umd",
          "multiple": true,
          "globals": {
            "@my-react/react": "React"
          },
          "name": "ReactDOMServer"
        }
      ]
    }
  ],
  "keywords": [
    "react-dom",
    "react-dom like",
    "react-dom server",
    "react-dom client"
  ],
  "devDependencies": {
    "@my-react/react-shared": "^0.3.10",
    "@my-react/react-reconciler": "^0.3.10"
  },
  "peerDependencies": {
    "@my-react/react": ">=0.3.10"
  }
}
