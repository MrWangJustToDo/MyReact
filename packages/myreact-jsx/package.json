{
  "name": "@my-react/react-jsx",
  "description": "@my-react/react-jsx",
  "author": "MrWangJustToDo",
  "license": "MIT",
  "version": "0.2.0",
  "main": "index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist",
    "index.js",
    "jsx-runtime.js",
    "jsx-runtime.d.ts",
    "jsx-dev-runtime.js",
    "jsx-dev-runtime.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MrWangJustToDo/MyReact",
    "directory": "packages/myreact-jsx"
  },
  "homepage": "https://mrwangjusttodo.github.io/MyReact",
  "exports": {
    ".": {
      "require": "./index.js",
      "node": "./index.js",
      "import": "./dist/esm/index.js",
      "default": "./index.js"
    },
    "./jsx-runtime": {
      "import": "./dist/esm/index.js",
      "node": "./jsx-runtime.js",
      "default": "./jsx-runtime.js",
      "types": "./jsx-runtime.d.ts"
    },
    "./jsx-dev-runtime": {
      "import": "./dist/esm/index.js",
      "node": "./jsx-dev-runtime.js",
      "default": "./jsx-dev-runtime.js",
      "types": "./jsx-dev-runtime.d.ts"
    }
  },
  "buildOptions": {
    "input": "./src/index.ts",
    "output": [
      {
        "dir": "./dist",
        "entryFileNames": "cjs/index.js",
        "format": "cjs",
        "multiple": true,
        "sourcemap": true
      },
      {
        "dir": "./dist",
        "entryFileNames": "esm/index.js",
        "format": "esm",
        "sourcemap": true
      }
    ]
  },
  "dependencies": {
    "@my-react/react-shared": "latest"
  },
  "peerDependencies": {
    "@my-react/react": ">=0.2.0"
  }
}
