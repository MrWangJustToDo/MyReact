{
  "name": "@my-react/react-jsx",
  "description": "@my-react/react-jsx",
  "version": "0.0.6",
  "main": "index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist",
    "index.js",
    "jsx-runtime.d.ts",
    "jsx-dev-runtime.d.ts"
  ],
  "exports": {
    ".": {
      "require": "./index.js",
      "node": "./index.js",
      "import": "./dist/esm/index.js",
      "default": "./index.js"
    },
    "./jsx-runtime": {
      "import": "./dist/esm/index.js",
      "node": "./index.js",
      "default": "./index.js",
      "types": "./jsx-runtime.d.ts"
    },
    "./jsx-dev-runtime": {
      "import": "./dist/esm/index.js",
      "node": "./index.js",
      "default": "./index.js",
      "types": "./jsx-dev-runtime.d.ts"
    }
  },
  "buildOptions": {
    "input": "./src/index.ts",
    "output": [
      {
        "dir": "./dist",
        "entryFileNames": "cjs/index.js",
        "format": "cjs",
        "multiple": true,
        "sourcemap": true
      },
      {
        "dir": "./dist",
        "entryFileNames": "esm/index.js",
        "format": "esm",
        "sourcemap": true
      }
    ]
  },
  "dependencies": {
    "@my-react/react": "latest",
    "@my-react/react-shared": "latest"
  }
}
