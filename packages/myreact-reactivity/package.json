{
  "name": "@my-react/react-reactive",
  "description": "@my-react/react-reactive",
  "version": "0.1.8",
  "main": "index.js",
  "types": "./dist/types/index.d.ts",
  "unpkg": "./dist/umd/index.development.js",
  "files": [
    "dist",
    "index.js"
  ],
  "exports": {
    "node": "./index.js",
    "require": "./index.js",
    "default": "./index.js"
  },
  "buildOptions": {
    "input": "./src/index.ts",
    "output": [
      {
        "dir": "./dist",
        "entryFileNames": "cjs/index.js",
        "format": "cjs",
        "multiple": true,
        "sourcemap": true
      },
      {
        "dir": "./dist",
        "entryFileNames": "esm/index.js",
        "format": "esm",
        "sourcemap": true
      },
      {
        "dir": "./dist",
        "entryFileNames": "umd/index.js",
        "format": "umd",
        "multiple": true,
        "globals": {
          "@my-react/react": "React"
        },
        "name": "ReactReactive",
        "sourcemap": true
      }
    ]
  },
  "dependencies": {
    "@my-react/react": "latest",
    "@my-react/react-shared": "latest"
  }
}
