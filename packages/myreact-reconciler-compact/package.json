{
  "name": "@my-react/react-reconciler-compact",
  "description": "@my-react/react-reconciler-compact",
  "author": "MrWangJustToDo",
  "license": "MIT",
  "version": "0.0.13",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "dist",
    "type.d.ts",
    "index.js",
    "index.d.ts",
    "constants.js",
    "constants.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MrWangJustToDo/MyReact",
    "directory": "packages/myreact-reconciler-compact"
  },
  "homepage": "https://mrwangjusttodo.github.io/MyReact",
  "exports": {
    ".": {
      "require": "./index.js",
      "import": "./dist/esm/index.mjs",
      "types": "./index.d.ts"
    },
    "./constants": {
      "require": "./constants.js",
      "import": "./dist/esm/constants.mjs",
      "types": "./constants.d.ts"
    },
    "./type": {
      "require": "./type.js",
      "types": "./type.d.ts"
    },
    "./package.json": "./package.json"
  },
  "buildOptions": [
    {
      "input": "./src/index.ts",
      "output": [
        {
          "dir": "./dist",
          "entryFileNames": "cjs/index.js",
          "format": "cjs",
          "type": true,
          "multiple": true
        },
        {
          "dir": "./dist",
          "entryFileNames": "esm/index.mjs",
          "format": "esm"
        }
      ]
    },
    {
      "input": "./src/constants.ts",
      "output": [
        {
          "dir": "./dist",
          "entryFileNames": "cjs/constants.js",
          "format": "cjs",
          "type": true,
          "multiple": true
        },
        {
          "dir": "./dist",
          "entryFileNames": "esm/constants.mjs",
          "format": "esm"
        }
      ]
    }
  ],
  "devDependencies": {
    "@my-react/react-reconciler": "^0.3.21",
    "@my-react/react-shared": "^0.3.21",
    "@types/react-reconciler": "^0.32.0"
  },
  "peerDependencies": {
    "@my-react/react": ">=0.3.21"
  }
}
