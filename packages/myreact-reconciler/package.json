{
  "name": "@my-react/react-reconciler",
  "description": "@my-react/react-reconciler",
  "version": "0.1.0",
  "main": "index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MrWangJustToDo/MyReact/tree/main/packages/myreact-reconciler"
  },
  "exports": {
    "node": "./index.js",
    "require": "./index.js",
    "default": "./index.js"
  },
  "buildOptions": {
    "input": "./src/index.ts",
    "output": [
      {
        "dir": "./dist",
        "entryFileNames": "cjs/index.js",
        "format": "cjs",
        "multiple": true,
        "sourcemap": true
      },
      {
        "dir": "./dist",
        "entryFileNames": "esm/index.js",
        "format": "esm",
        "sourcemap": true
      }
    ]
  },
  "dependencies": {
    "@my-react/react": "latest",
    "@my-react/react-shared": "latest",
    "lodash": "^4.17.21"
  }
}
