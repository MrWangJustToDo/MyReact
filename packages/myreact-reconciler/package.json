      },
      {
        "dir": "./dist",
        "entryFileNames": "esm/index.js",
        "format": "esm",
        "sourcemap": true
      }
    ]
  },
  "dependencies": {
    "@my-react/react-shared": "latest"
  },
  "peerDependencies": {
    "@my-react/react": ">=0.2.2"
  }
