  "license": "MIT",
  "version": "0.2.4",
  "main": "index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MrWangJustToDo/MyReact",
    "directory": "packages/myreact-reconciler"
  },
  "homepage": "https://mrwangjusttodo.github.io/MyReact",
  "exports": {
    "node": "./index.js",
    "require": "./index.js",
    "default": "./index.js"
  },
  "buildOptions": {
    "input": "./src/index.ts",
    "output": [
      {
        "dir": "./dist",
        "entryFileNames": "cjs/index.js",
