{
  "name": "@my-react/react-refresh",
  "description": "@my-react/react-refresh",
  "author": "MrWangJustToDo",
  "license": "MIT",
  "version": "0.2.2",
  "main": "runtime.js",
  "types": "runtime.d.ts",
  "files": [
    "dist",
    "babel.js",
    "runtime.js",
    "runtime.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MrWangJustToDo/MyReact",
    "directory": "packages/myreact-refresh"
  },
  "exports": {
    ".": {
      "require": "./runtime.js",
      "types": "./runtime.d.ts"
    },
    "./runtime": {
      "require": "./runtime.js",
      "types": "./runtime.d.ts"
    },
    "./babel": "./babel.js",
    "./package.json": "./package.json"
  },
  "buildOptions": [
    {
      "input": "./src/RefreshBabelPlugin.ts",
      "pkgName": "RefreshBabelPlugin",
      "output": [
        {
          "dir": "./dist",
          "entryFileNames": "cjs/RefreshBabelPlugin.js",
          "format": "cjs",
          "multiple": true,
          "sourcemap": true
        },
        {
          "dir": "./dist",
          "entryFileNames": "esm/RefreshBabelPlugin.js",
          "format": "esm",
          "sourcemap": true
        }
      ]
    },
    {
      "input": "./src/RefreshRuntime.ts",
      "pkgName": "RefreshRuntime",
      "output": [
        {
          "dir": "./dist",
          "entryFileNames": "cjs/RefreshRuntime.js",
          "format": "cjs",
          "multiple": true,
          "sourcemap": true
        },
        {
          "dir": "./dist",
          "entryFileNames": "esm/RefreshRuntime.js",
          "format": "esm",
          "sourcemap": true
        }
      ]
    }
  ],
  "dependencies": {
    "@my-react/react-shared": "latest"
  },
  "peerDependencies": {
    "@my-react/react": ">=0.2.2",
    "@my-react/react-dom": ">=0.2.2"
  }
}
