{
  "name": "@my-react/react-refresh",
  "description": "@my-react/react-refresh",
  "version": "0.0.1",
  "main": "runtime.js",
  "files": [
    "src",
    "babel.js",
    "runtime.js"
  ],
  "exports": {
    ".": "./runtime.js",
    "./runtime": "./runtime.js",
    "./babel": "./babel.js",
    "./package.json": "./package.json"
  },
  "buildOptions": [
    {
      "input": "./src/RefreshBabelPlugin.ts",
      "output": [
        {
          "dir": "./dist",
          "entryFileNames": "cjs/RefreshBabelPlugin.js",
          "format": "cjs",
          "multiple": true,
          "sourcemap": true
        },
        {
          "dir": "./dist",
          "entryFileNames": "esm/RefreshBabelPlugin.js",
          "format": "esm",
          "sourcemap": true
        }
      ]
    },
    {
      "input": "./src/RefreshRuntime.ts",
      "output": [
        {
          "dir": "./dist",
          "entryFileNames": "cjs/RefreshRuntime.js",
          "format": "cjs",
          "multiple": true,
          "sourcemap": true
        },
        {
          "dir": "./dist",
          "entryFileNames": "esm/RefreshRuntime.js",
          "format": "esm",
          "sourcemap": true
        }
      ]
    }
  ],
  "dependencies": {
    "@my-react/react": "latest",
    "@my-react/react-shared": "latest",
    "@my-react/react-reconciler": "latest"
  }
}
