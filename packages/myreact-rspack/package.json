{
  "name": "@my-react/react-rspack",
  "version": "0.0.16",
  "description": "make @my-react work for rspack",
  "files": [
    "dist",
    "client",
    "exports",
    "index.js",
    "overlay.js",
    "react-refresh.js",
    "react-refresh-entry.js"
  ],
  "license": "MIT",
  "main": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "require": "./exports/index.cjs",
      "default": "./exports/index.mjs"
    },
    "./react-refresh": "./react-refresh.js",
    "./react-refresh-entry": "./react-refresh-entry.js",
    "./overlay": "./overlay.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc -b ./tsconfig.build.json",
    "dev": "tsc -b -w"
  },
  "peerDependencies": {
    "@my-react/react-refresh": ">=0.3.21",
    "webpack-hot-middleware": "2.x"
  },
  "dependencies": {
    "error-stack-parser": "^2.1.4",
    "html-entities": "^2.6.0"
  },
  "devDependencies": {
    "@my-react/react-shared": "^0.3.21",
    "@rspack/core": "^1.3.2"
  }
