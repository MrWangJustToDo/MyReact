{
  "name": "@my-react/react-terminal",
  "description": "@my-react/react-terminal",
  "author": "MrWangJustToDo",
  "license": "MIT",
  "version": "0.0.16",
  "main": "index.mjs",
  "types": "./index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "index.mjs",
    "index.d.ts",
    "jsx-runtime.js",
    "jsx-runtime.d.ts",
    "jsx-dev-runtime.js",
    "jsx-dev-runtime.d.ts",
    "jsx-namespace.d.ts",
    "package.json"
  ],
  "exports": {
    ".": {
      "import": "./index.mjs",
      "types": "./index.d.ts"
    },
    "./jsx-runtime": {
      "import": "./jsx-runtime.js",
      "types": "./jsx-runtime.d.ts"
    },
    "./jsx-dev-runtime": {
      "import": "./jsx-dev-runtime.js",
      "types": "./jsx-dev-runtime.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MrWangJustToDo/MyReact",
    "directory": "packages/myreact-terminal"
  },
  "buildOptions": {
    "input": "./src/index.ts",
    "output": [
      {
        "dir": "./dist",
        "entryFileNames": "esm/index.mjs",
        "format": "esm",
        "type": true
      }
    ]
  },
  "dependencies": {
    "@alcalzone/ansi-tokenize": "^0.2.0",
    "ansi-escapes": "^7.0.0",
    "ansi-styles": "^6.2.3",
    "auto-bind": "^5.0.1",
    "chalk": "^5.6.2",
    "cli-boxes": "^3.0.0",
    "cli-cursor": "^4.0.0",
    "cli-truncate": "^4.0.0",
    "code-excerpt": "^4.0.0",
    "es-toolkit": "^1.40.0",
    "indent-string": "^5.0.0",
    "is-in-ci": "^2.0.0",
    "mnemonist": "^0.40.3",
    "patch-console": "^2.0.0",
    "signal-exit": "^4.1.0",
    "slice-ansi": "^7.1.2",
    "socket.io-client": "^4.8.1",
    "stack-utils": "^2.0.6",
    "string-width": "^7.2.0",
    "type-fest": "^4.41.0",
    "widest-line": "^5.0.0",
    "wrap-ansi": "^9.0.2",
    "yoga-layout": "^3.2.1"
  },
  "devDependencies": {
    "@types/react-reconciler": "^0.32.0",
    "@types/signal-exit": "^4.0.0",
    "@types/slice-ansi": "^7.1.0",
    "@types/stack-utils": "^2.0.3"
  },
  "peerDependencies": {
    "@my-react/react": ">=0.3.22"
  }
}
