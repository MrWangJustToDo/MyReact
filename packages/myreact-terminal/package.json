{
  "name": "@my-react/react-terminal",
  "description": "@my-react/react-terminal",
  "author": "MrWangJustToDo",
  "license": "MIT",
  "version": "0.1.9",
  "main": "index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MrWangJustToDo/MyReact",
    "directory": "packages/myreact-terminal"
  },
  "exports": {
    "node": "./index.js",
    "require": "./index.js",
    "default": "./index.js"
  },
  "buildOptions": {
    "input": "./src/index.ts",
    "output": [
      {
        "dir": "./dist",
        "entryFileNames": "cjs/index.js",
        "format": "cjs",
        "multiple": true,
        "sourcemap": true
      },
      {
        "dir": "./dist",
        "entryFileNames": "esm/index.js",
        "format": "esm",
        "sourcemap": true
      }
    ]
  },
  "dependencies": {
    "ansi-escapes": "^6.2.0",
    "auto-bind": "^5.0.1",
    "chalk": "^5.2.0",
    "cli-boxes": "^3.0.0",
    "cli-cursor": "^4.0.0",
    "cli-truncate": "^3.1.0",
    "code-excerpt": "^4.0.0",
    "indent-string": "^5.0.0",
    "lodash": "^4.17.21",
    "patch-console": "^2.0.0",
    "signal-exit": "^4.0.2",
    "slice-ansi": "^6.0.0",
    "stack-utils": "^2.0.6",
    "string-width": "^6.1.0",
    "type-fest": "^3.10.0",
    "widest-line": "^4.0.1",
    "wrap-ansi": "^8.1.0",
    "yoga-wasm-web": "^0.3.3"
  },
  "devDependencies": {
    "@my-react/react-shared": "^0.2.8",
    "@my-react/react-reconciler": "^0.2.8",
    "@types/lodash": "^4.14.202",
    "@types/signal-exit": "^3.0.1",
		"@types/slice-ansi": "^5.0.0",
		"@types/stack-utils": "^2.0.1",
		"@types/wrap-ansi": "^8.0.1"
  },
  "peerDependencies": {
    "@my-react/react": ">=0.2.8"
  }
}
