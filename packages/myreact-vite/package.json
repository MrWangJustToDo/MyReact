{
  "name": "@my-react/react-vite",
  "version": "0.0.3",
  "description": "make @my-react work for vite",
  "files": [
    "dist",
    "index.js"
  ],
  "license": "MIT",
  "main": "index.js",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "node": "./index.js",
      "require": "./index.js"
    },
    "./refreshUtils": {
      "types": "./dist/types/refreshUtils.d.ts",
      "require": "./refreshUtils.js"
    },
    "./package.json": "./package.json"
  },
  "buildOptions": [
    {
      "input": "./src/index.ts",
      "output": [
        {
          "dir": "./dist",
          "entryFileNames": "cjs/index.js",
          "format": "cjs",
          "multiple": true,
          "sourcemap": true
        },
        {
          "dir": "./dist",
          "entryFileNames": "esm/index.mjs",
          "format": "esm",
          "sourcemap": true
        }
      ]
    },
    {
      "input": "./src/refreshUtils.ts",
      "pkgName": "refreshUtils",
      "output": [
        {
          "dir": "./dist",
          "entryFileNames": "cjs/refreshUtils.js",
          "format": "cjs",
          "multiple": true,
          "sourcemap": true
        }
      ]
    }
  ],
  "peerDependencies": {
    "@my-react/react-refresh": ">=0.2.9",
    "vite": "^4.2.0 || ^5.0.0"
  },
  "dependencies": {
    "@babel/core": "^7.22.5",
    "@babel/plugin-transform-react-jsx-self": "^7.22.5",
    "@babel/plugin-transform-react-jsx-source": "^7.22.5"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.1"
  }
}
