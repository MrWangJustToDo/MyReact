{
  "name": "@my-react/react",
  "description": "@my-react/react",
  "version": "0.1.2",
  "main": "index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist",
    "index.js",
    "jsx-main.js",
    "jsx-runtime.d.ts",
    "jsx-dev-runtime.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MrWangJustToDo/MyReact/tree/main/packages/myreact"
  },
  "exports": {
    ".": {
      "require": "./index.js",
      "node": "./index.js",
      "default": "./index.js"
    },
    "./jsx-runtime": {
      "require": "./jsx-main.js",
      "types": "./jsx-runtime.d.ts"
    },
    "./jsx-dev-runtime": {
      "require": "./jsx-main.js",
      "types": "./jsx-dev-runtime.d.ts"
    }
  },
  "buildOptions": {
    "input": "./src/index.ts",
    "output": [
      {
        "dir": "./dist",
        "entryFileNames": "cjs/index.js",
        "format": "cjs",
        "multiple": true,
        "sourcemap": true
      },
      {
        "dir": "./dist",
        "entryFileNames": "esm/index.js",
        "format": "esm",
        "sourcemap": true
      },
      {
        "dir": "./dist",
        "entryFileNames": "umd/index.js",
        "format": "umd",
        "multiple": true,
        "name": "React",
        "sourcemap": true
      }
    ]
  },
  "dependencies": {
    "@my-react/react-jsx": "latest",
    "@my-react/react-shared": "latest",
    "@my-react/react-reactive": "latest"
  }
}
