{
  "name": "@my-react/react",
  "description": "@my-react/react",
  "author": "MrWangJustToDo",
  "license": "MIT",
  "version": "0.2.2",
  "main": "index.js",
  "types": "./dist/types/index.d.ts",
  "unpkg": "./dist/umd/index.development.js",
  "jsdelivr": "./dist/umd/index.development.js",
  "files": [
    "dist",
    "index.js",
    "jsx-runtime.js",
    "jsx-runtime.d.ts",
    "jsx-dev-runtime.js",
    "jsx-dev-runtime.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MrWangJustToDo/MyReact",
    "directory": "packages/myreact"
  },
  "homepage": "https://mrwangjusttodo.github.io/MyReact",
  "exports": {
    ".": {
      "require": "./index.js",
      "node": "./index.js",
      "default": "./index.js"
    },
    "./jsx-runtime": {
      "import": "./jsx-runtime.js",
      "require": "./jsx-runtime.js",
      "node": "./jsx-runtime.js",
      "types": "./jsx-runtime.d.ts"
    },
    "./jsx-dev-runtime": {
      "import": "./jsx-dev-runtime.js",
      "require": "./jsx-dev-runtime.js",
      "node": "./jsx-dev-runtime.js",
      "types": "./jsx-dev-runtime.d.ts"
    },
    "./package.json": "./package.json"
  },
  "buildOptions": {
    "input": "./src/index.ts",
    "output": [
      {
        "dir": "./dist",
        "entryFileNames": "cjs/index.js",
        "format": "cjs",
        "multiple": true,
        "sourcemap": true
      },
      {
        "dir": "./dist",
        "entryFileNames": "esm/index.js",
        "format": "esm",
        "sourcemap": true
      },
      {
        "dir": "./dist",
        "entryFileNames": "umd/index.js",
        "format": "umd",
        "multiple": true,
        "name": "React",
        "sourcemap": true
      }
    ]
  },
  "keywords": [
    "react",
    "react like"
  ],
  "dependencies": {
    "@my-react/react-jsx": "latest",
    "@my-react/react-shared": "latest"
  }
}
