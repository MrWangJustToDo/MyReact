{
  "name": "@my-react/react",
  "description": "@my-react/react",
  "version": "0.0.1",
  "main": "index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist",
    "index.js"
  ],
  "exports": {
    ".": {
      "require": "./index.js",
      "import": "./dist/esm/index.js",
      "default": "./index.js"
    },
    "./jsx-runtime": "./index.js",
    "./jsx-dev-runtime": "./index.js",
    "./package.json": "./package.json"
  },
  "buildOptions": {
    "input": "./src/index.ts",
    "output": [
      {
        "dir": "./dist",
        "entryFileNames": "cjs/index.js",
        "esModule": false,
        "format": "cjs",
        "multiple": true,
        "sourcemap": true
      },
      {
        "dir": "./dist",
        "entryFileNames": "esm/index.js",
        "format": "esm",
        "sourcemap": true
      },
      {
        "dir": "./dist",
        "entryFileNames": "browser/index.js",
        "format": "esm",
        "multiple": true,
        "sourcemap": true
      },
      {
        "dir": "./dist",
        "entryFileNames": "umd/index.js",
        "format": "umd",
        "esModule": false,
        "multiple": true,
        "name": "React",
        "sourcemap": true
      }
    ]
  }
}
